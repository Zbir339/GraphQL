type Query {
    ## this are the queries you have
    ## !! this queries must have the same name in your controller and same returning type
    books: [Book]
    bookById(id: ID): Book
}
### this below are schemas
type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}